/ {
  keymap {
    compatible = "zmk,keymap";

    default_layer {
      bindings = <
        &kp A &kp B &kp C &kp D
        &kp E &kp F &kp G &kp H
        &kp I &kp J &kp K &kp L
        &kp M &kp N &kp O &kp P
      >;
    };

    layer_1 {
      bindings = <
        &kp 1 &kp 2 &kp 3 &kp 4
        &kp 5 &kp 6 &kp 7 &kp 8
        &kp 9 &kp 0 &kp MINUS &kp EQUAL
        &kp LCTRL &kp LALT &kp TAB &kp ESC
      >;
    };
  };
};

